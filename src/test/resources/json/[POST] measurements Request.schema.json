				"default": 0
			},
			"temperature": {
				"$id": "#root/items/temperature", 
				"title": "Temperature", 
				"type": "number",
				"examples": [
					18.7
				],
				"default": 0.0
			},
			"wind_speed": {
				"$id": "#root/items/wind_speed", 
				"title": "Wind_speed", 
				"type": "number",
				"examples": [
					1.2
				],
				"default": 0.0
			},
			"wind_gust": {
				"$id": "#root/items/wind_gust", 
				"title": "Wind_gust", 
				"type": "number",
				"examples": [
					3.4
				],
				"default": 0.0
			},
			"pressure": {
				"$id": "#root/items/pressure", 
				"title": "Pressure", 
