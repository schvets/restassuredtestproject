{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1588834162.json", 
	"title": "Root", 
	"type": "array",
	"default": [],
	"items":{
		"$id": "#root/items", 
		"title": "Items", 
		"type": "object",
		"required": [
			"station_id",
			"dt",
			"temperature",
			"wind_speed",
			"wind_gust",
			"pressure",
			"humidity",
			"rain_1h",
			"clouds"
		],
		"properties": {
			"station_id": {
				"$id": "#root/items/station_id", 
				"title": "Station_id", 
				"type": "string",
				"default": "",
				"examples": [
					"583436dd9643a9000196b8d6"
				],
				"pattern": "^.*$"
			},
			"dt": {
				"$id": "#root/items/dt", 
				"title": "Dt", 
				"type": "integer",
